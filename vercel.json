{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key",
    "LLM_PROVIDER": "openai",
    "LLM_MODEL": "gpt-3.5-turbo",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}

